                  <input type="checkbox" name ="used_drugs_pregnancy" value="tobacco" checked>&nbsp;Tobacco (vaping, chewing, etc.)&nbsp;&nbsp;
                <% } else { %>
                  <input type="checkbox" name ="used_drugs_pregnancy" value="tobacco">&nbsp;Tobacco (vaping, chewing, etc.)&nbsp;&nbsp;
                <% } if (pregbirth.usedDrugsPregnancy == "marijuana") { %>
